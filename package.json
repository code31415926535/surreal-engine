{
  "name": "surreal-engine",
  "description": "A typescript game engine",
  "keywords": [
    "typescript",
    "game",
    "engine",
    "three",
    "threejs",
    "three.js",
    "ammo",
    "ammo.js",
    "ammojs"
  ],
  "private": false,
  "version": "0.1.0",
  "homepage": "https://github.com/code31415926535/surreal-engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/code31415926535/surreal-engine.git"
  },
  "bugs": {
    "url": "https://github.com/code31415926535/surreal-engine/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/surreal-engine.umd.js",
  "module": "dist/surreal-engine.es.js",
  "types": "dist/surreal-engine.d.ts",
  "exports": {
    ".": {
      "import": "./dist/surreal-engine.es.js",
      "require": "./dist/surreal-engine.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "npm run build:module && npm run build:types",
    "build:module": "tsc && vite build && rm -rf ./dist/assets",
    "build:types": "tsc -m amd -t ESNext -d --isolatedModules false --moduleResolution node --emitDeclarationOnly --outFile dist/index src/lib/index.ts",
    "build:examples": "tsc && vite --config vite.examples.config.js build"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.139.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.0"
  },
  "dependencies": {
    "ammojs-typed": "^1.0.6",
    "nanoid": "^3.3.3",
    "three": "^0.139.2",
    "tick-knock": "^4.0.2"
  }
}
